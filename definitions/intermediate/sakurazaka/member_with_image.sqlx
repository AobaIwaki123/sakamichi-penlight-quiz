config {
  type: "table",
  schema: "sakurazaka",
  name: "member_with_image",
  description: "メンバー情報に画像を結合したもの",
}

SELECT
  info.id,
  info.name,
  info.nickname,
  info.emoji,
  info.gen,
  info.graduated,
  info.penlight1,
  info.penlight2,
  img.type,
  img.url
FROM
  ${ref("sakurazaka", "member_info")} info
JOIN
  ${ref("sakurazaka", "member_image_master")} img
ON
  info.name = img.name
